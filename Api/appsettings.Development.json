{
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Domain": "Debug",
        "Microsoft.AspNetCore": "Warning"
      },
      "FormatterName": "jsoin",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Idented": true
        }
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Information"
      }
    }
  }
}
